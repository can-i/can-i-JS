{
  "name": "can-i",
  "version": "1.1.0",
  "author": "Shavauhn Gabay",
  "bugs": {
    "url": "https://github.com/can-i/can-i-JS/issues"
  },
  "dependencies": {
    "express": "^4.14.0",
    "limberjack": "^2.2.6",
    "mkdirp": "^0.5.1",
    "promise-lib": "^0.1.3",
    "require-dir": "^0.3.1",
    "tslib": "^1.5.0",
    "unhandled-rejection": "^1.0.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/express": "^4.0.35",
    "@types/gulp": "^3.8.32",
    "@types/mkdirp": "^0.3.29",
    "@types/mocha": "^2.2.38",
    "@types/sinon": "^1.16.34",
    "chai": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "mocha": "^3.2.0",
    "pump": "^1.0.2",
    "run-sequence": "^1.2.2",
    "source-map-support": "^0.4.11",
    "ts-glob": "0.0.9",
    "typedoc": "^0.5.5",
    "typescript": "^2.1.5"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/can-i/can-i-JS#readme",
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/can-i/can-i-JS.git"
  },
  "scripts": {
    "build": "gulp",
    "doc:generation": "typedoc --tsconfig doc.json --ignoreCompilerErrors --out _book/classes  --excludeExternals --excludePrivate --mode file",
    "test": "mocha"
  },
  "ts": [
    {
      "config": "tsconfig.json",
      "files": [
        "gulpfile.ts",
        "index.ts",
        "Framework/**/*.ts",
        "scripts/**/*.ts",
        "test/**/*.ts"
      ],
      "exclude": "/d\\.ts$/"
    },
    {
      "config": "doc.json",
      "files": [
        "index.ts",
        "Framework/**/*.ts"
      ],
      "exclude": "/d\\.ts$/"
    }
  ]
}
